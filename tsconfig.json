{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "esModuleInterop": true,
    "jsx": "react",
    "sourceMap": true,
    "target": "es6",
    "moduleResolution": "node",

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true
  }
}
